{
  "name": "guidebook-web-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "dev-social": "VITE_API_URL=http://localhost:3000 react-router dev",
    "typecheck": "tsc --noEmit",
    "typegen": "react-router typegen",
    "lint": "eslint app/ --max-warnings 0",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "generate:client": "swagger-typescript-api generate -p ../api/src/swagger/api-schema.json -o ./app/api --axios --name generated-api.ts --api-class-name API",
    "start": "react-router-serve ./build/server/index.js",
    "tsc": "react-router typegen && tsc",
    "test": "vitest"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-router/node": "^7.9.0",
    "@react-router/serve": "^7.9.0",
    "@tanstack/react-query": "^5.87.4",
    "axios": "^1.12.1",
    "better-auth": "^1.3.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "i18next": "^25.5.2",
    "isbot": "^5.1.27",
    "lucide-react": "^0.544.0",
    "next-themes": "^0.4.6",
    "nuqs": "^2.6.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.62.0",
    "react-i18next": "^16.0.0",
    "react-router": "^7.9.0",
    "sonner": "^1.7.4",
    "tailwind-merge": "^2.6.0",
    "zod": "^4.1.8",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@react-router/dev": "^7.9.0",
    "@repo/eslint-config": "workspace:*",
    "@tailwindcss/vite": "^4.1.4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.19.13",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "eslint": "^9.36.0",
    "jsdom": "^27.0.0",
    "prettier-plugin-tailwindcss": "^0.7.0",
    "swagger-typescript-api": "^13.2.11",
    "tailwindcss": "^4.1.4",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5.8.3",
    "vite": "^7.1.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
